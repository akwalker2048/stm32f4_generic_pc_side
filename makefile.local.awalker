#Local Compiler Defines
#This is a PC application...not cross compiled...we will just use the default
#gcc and binutils...
EXE = pc_comm

CC      = gcc
LD      = gcc
CP      = objcopy
OD      = objdump
AS      = as

GENERIC_PACKET_SRC_DIR = ../stm32f4_generic_packet/src
GENERIC_PACKET_INC_DIR = ../stm32f4_generic_packet/include

LOCAL_CFLAGS = -D"PC_DEBUG_ON"
